{
  "name": "ant-design-pro",
  "version": "4.2.1",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.10.71",
    "@ant-design/pro-layout": "^6.38.0",
    "@ant-design/pro-table": "^2.74.1",
    "@antv/data-set": "^0.11.8",
    "@microsoft/signalr": "^6.0.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isequal": "^4.5.6",
    "bizcharts": "^3.5.10",
    "bizcharts-plugin-slider": "^2.1.1",
    "@umijs/route-utils": "^2.0.5",
    "antd": "^4.20.4",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.29.3",
    "numeral": "^2.0.6",
    "oidc-client": "^1.11.5",
    "omit.js": "^2.0.2",
    "qs": "^6.10.3",
    "rc-util": "^5.21.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.3.0",
    "react-selecto": "^1.16.2",
    "reqwest": "^2.0.5",
    "selecto": "^1.16.2",
    "umi": "^3.5.23",
    "umi-request": "^1.4.0",
    "use-merge-value": "^1.0.2",
    "xml-formatter": "^2.6.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/classnames": "^2.3.0",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@umijs/fabric": "^2.10.2",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.31",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "gh-pages": "^4.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.4.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^14.0.0",
    "stylelint": "^14.8.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
